<local:ModuleBaseDlg x:Class="BrainSimulator.Modules.ModuleTextFileDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainSimulator.Modules"
        mc:Ignorable="d"
        Title="Handle UKS Txt file Format" Height="170" Width="560" Background="Gray" ShowInTaskbar="False" >

    <Grid Margin="14">

        <TextBlock x:Name="StatusText" Margin="0,0,0,100" ></TextBlock>
        <Button x:Name="ImportButton" Content="Import" Width="90" IsDefault="True" Click="ImportButton_Click" Height="29" Margin="99,44,343,0" HorizontalAlignment="Center" VerticalAlignment="Top" />
        <Button x:Name="ExportButton" Content="Export" Width="90" IsDefault="False" Click="ExportButton_Click" Height="29" Margin="221,44,221,0" HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Grid>
</local:ModuleBaseDlg>
