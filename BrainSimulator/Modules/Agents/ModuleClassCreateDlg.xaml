<local:ModuleBaseDlg x:Class="BrainSimulator.Modules.ModuleClassCreateDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainSimulator.Modules"
        mc:Ignorable="d"
        Title="Class Create Agent" Height="250" Width="300" Background="Gray" ShowInTaskbar="False">
    <Grid Name="theGrid" SizeChanged="TheGrid_SizeChanged" >
        <Button Content="Run Once" HorizontalAlignment="Left" Height="30" Margin="100,0,0,29" VerticalAlignment="bottom" Width="105" Click="Button_Click"/>
        <TextBox x:Name="tbMessages" Margin="25,24,20,86" TextWrapping="Wrap" Text=""/>
        <CheckBox x:Name="Enable" HorizontalAlignment="Left" Margin="18,0,0,34" Content="Enabled" IsChecked="False" VerticalAlignment="Bottom" Checked="Enable_Checked" Unchecked="Enable_Checked" />
    </Grid>
</local:ModuleBaseDlg>
