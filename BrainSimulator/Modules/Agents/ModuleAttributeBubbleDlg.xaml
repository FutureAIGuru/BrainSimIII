<local:ModuleBaseDlg x:Class="BrainSimulator.Modules.ModuleAttributeBubbleDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrainSimulator.Modules"
        mc:Ignorable="d"
        Title="Attribute Bubble Agent" Height="250" Width="530" Background="Gray" ShowInTaskbar="False">
    <Grid Name="theGrid" SizeChanged="TheGrid_SizeChanged" >
        <Button Content="Run Once" HorizontalAlignment="Left" Margin="100,0,0,29" Width="105" Click="Button_Click" Height="30" VerticalAlignment="Bottom"/>
        <TextBox x:Name="tbMessages" Margin="25,24,20,86" TextWrapping="Wrap" Text="" VerticalScrollBarVisibility="Auto"/>
        <CheckBox x:Name="Enable" HorizontalAlignment="Left" Margin="18,0,0,34" Content="Enabled" IsChecked="False" Checked="Enable_Checked" Unchecked="Enable_Checked" Height="19" VerticalAlignment="Bottom" />
    </Grid>
</local:ModuleBaseDlg>
